<% let data =[{
    "title": "Title from File Awesome JPG FILE",
    "subtitle": "Subtitle File Awesome JPG FILE",
    "filesize": 100,
    "unit": "mb",
    "url": "filename.jpg",
    "online": true
}, {
    "title": "Title from File Awesome PNG FILE",
    "subtitle": "Subtitle File Awesome PNG FILE",
    "filesize": 80,
    "unit": "mb",
    "url": "filename.png",
    "online": true
}, {
    "title": "Title from File Awesome EXE FILE",
    "subtitle": "Subtitle File Awesome EXE FILE",
    "filesize": 2,
    "unit": "gb",
    "url": "filename.exe",
    "online": false
}]%>

<table>
    <thead>
    <tr>
        <th>Status</th>
        <th>Filename</th>
        <th>Size</th>
    </tr>
    </thead>
    <tbody>
    <% data.forEach((obj) => { %>
        <tr>
            <td class="status"><i class='bx bxs-check-circle <%= obj.online ? "online" : "offline" %>'></i></td>
            <td title="<%= obj.title %>"><%= obj.title %>
                <span>
                    <a href="<%= obj.online ? obj.url : '#' %>" target="blank"><%= obj.subtitle %></a>
                </span>
            </td>
            <td class="file-size">
                <p><%= obj.filesize %></p>
                <span> <%= obj.unit.toUpperCase() %></span>
            </td>
            <td>
                <a class="button download <%= obj.online ? "online" : "offline" %>"
                   target="_blank"
                   title="download"
                   href="<%= obj.online ? obj.url : '#' %>">
                    <%= obj.online ? "Download" : "Offline" %>
                </a>
            </td>
        </tr>
    <% }) %>
    </tbody>
</table>